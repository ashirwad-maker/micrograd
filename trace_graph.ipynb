   "source": [
    "from micrograd import neural_network\n",
    "from micrograd.engine import Value\n",
    "from traceGraph import traceGraph \n",
    "n = neural_network.Neuron(2)\n",
    "x = [Value(1.0), Value(-2.0)]\n",
    "y = n(x)\n",
    "y.backward()\n",
    "traceGraph().draw_dot(y).render(\"treeImages/demo_graph\")"
   ]
  }
